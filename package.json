{
  "name": "files-and-folders",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "local:run": "node apps/api/main.js",
    "docker:kill": "docker kill files-and-folders",
    "docker:container-ls": "docker container ls",
    "docker:container-remove": "docker container rm files-and-folders && docker image rm krocon/files-and-folders -f",
    "docker:images": "docker images",
    "docker:image-remove": "docker image rm krocon/files-and-folders -f",
    "docker:build": "docker build . -t krocon/files-and-folders",
    "docker:push": "docker push krocon/files-and-folders:latest",
    "docker:run": "docker run -p 3333:3333 -p 3334:3334 -v /:/fnf --name fnf --env=FNF_INCOMPATIBLE_PATHS='' -d krocon/fnf",
    "docker:run-win": "docker run -p 3333:3333 -p 3334:3334 -v c:\\:/fnf/c  -v d:\\:/fnf/d --name fnf --env=FNF_INCOMPATIBLE_PATHS='' --env=FNF_START_PATHS='c:/' -d krocon/files-and-folders",
    "docker:run-imac": "docker run -p 3333:3333 -p 3334:3334 -v /Users/marckronberg:/fnf/vols/marckronberg -v /Users/Shared:/fnf/vols/Shared --name fnf --env=FNF_INCOMPATIBLE_PATHS='' -d krocon/files-and-folders",
    "docker-restart": "docker container start files-and-folders",
    "docker:print-container-id": "docker ps | grep  ' krocon/fnf ' | awk '{ print $1 }'",
    "docker:logs": "docker logs $(docker ps | grep  ' krocon/fnf ' | awk '{ print $1 }') ",
    "docker:logs-f": "docker logs $(docker ps | grep  ' krocon/fnf ' | awk '{ print $1 }') -f"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "13.1.1",
    "@angular/cdk": "13.1.1",
    "@angular/common": "13.1.1",
    "@angular/compiler": "13.1.1",
    "@angular/core": "13.1.1",
    "@angular/forms": "13.1.1",
    "@angular/material": "13.1.1",
    "@angular/platform-browser": "13.1.1",
    "@angular/platform-browser-dynamic": "13.1.1",
    "@angular/router": "13.1.1",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^8.2.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/websockets": "^8.0.10",
    "@nrwl/angular": "13.3.9",
    "@postfinance/ngx-ace-editor-wrapper": "^11.1.2",
    "ag-grid-angular": "^26.1.0",
    "ag-grid-community": "^26.1.0",
    "autoprefixer": "^10.4.0",
    "brace": "^0.11.1",
    "chokidar": "^3.5.2",
    "fs-extra": "^10.0.0",
    "hotkeys-js": "^3.8.7",
    "micromatch": "^4.0.4",
    "ngx-color-picker": "^11.0.0",
    "ngx-socket-io": "^4.1.0",
    "node-stream-zip": "^1.15.0",
    "postcss": "^8.3.11",
