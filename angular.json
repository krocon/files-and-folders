{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": [
        "scope:api"
      ]
    },
    "files-and-folders": {
      "projectType": "application",
      "root": "apps/files-and-folders",
      "sourceRoot": "apps/files-and-folders/src",
      "prefix": "fnf",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/files-and-folders",
            "index": "apps/files-and-folders/src/index.html",
            "main": "apps/files-and-folders/src/main.ts",
            "polyfills": "apps/files-and-folders/src/polyfills.ts",
            "tsConfig": "apps/files-and-folders/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/files-and-folders/src/favicon.ico",
              "apps/files-and-folders/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/files-and-folders/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "3kb",
                  "maximumError": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/files-and-folders/src/environments/environment.ts",
                  "with": "apps/files-and-folders/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "files-and-folders:build:production"
            },
            "development": {
              "browserTarget": "files-and-folders:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/files-and-folders/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "files-and-folders:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/files-and-folders/src/**/*.ts",
              "apps/files-and-folders/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/files-and-folders"
          ],
          "options": {
            "jestConfig": "apps/files-and-folders/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": [
        "scope:client"
      ]
    },
    "files-and-folders-e2e": {
      "root": "apps/files-and-folders-e2e",
      "sourceRoot": "apps/files-and-folders-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/files-and-folders-e2e/cypress.json",
            "devServerTarget": "files-and-folders:serve:development",
            "tsConfig": "apps/files-and-folders-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "files-and-folders:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/files-and-folders-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [
        "scope:client-e2e"
      ],
      "implicitDependencies": [
        "files-and-folders"
      ]
    },
    "fnf-data": {
      "projectType": "library",
      "root": "libs/fnf-data",
      "sourceRoot": "libs/fnf-data/src",
      "prefix": "fnf",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/fnf-data"
          ],
          "options": {
            "jestConfig": "libs/fnf-data/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/fnf-data/src/**/*.ts",
              "libs/fnf-data/src/**/*.html"
            ]
          }
        }
      },
      "tags": [
        "scope:fnf-data"
      ]
    }
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
